<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="600.0" prefWidth="800.0" spacing="30.0" style="-fx-padding: 30" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.banco.controlador.PanelClienteControlador">
   <children>
      <Label text="Panel Principal de administración" />
      <HBox prefHeight="100.0" prefWidth="200.0" spacing="500.0">
         <children>
            <Label fx:id="LabelDeBievenida" text="Bievenida..." />
            <Label fx:id="NumeroDeCuentaLabel" text="Numero de cuenta" />
         </children>
      </HBox>
      <TableView fx:id="TablaTransacciones" prefHeight="350.0" prefWidth="740.0">
        <columns>
          <TableColumn fx:id="ColTipo" prefWidth="75.0" text="Tipo" />
          <TableColumn fx:id="ColFecha" prefWidth="75.0" text="Fecha" />
            <TableColumn fx:id="ColValor" prefWidth="75.0" text="Valor" />
            <TableColumn fx:id="ColUsuario" prefWidth="75.0" text="Usuario" />
            <TableColumn fx:id="ColCategoria" prefWidth="75.0" text="Categoria" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox prefHeight="73.0" prefWidth="740.0" spacing="20.0">
         <children>
            <Button fx:id="ButtonCerrarSesion" mnemonicParsing="false" text="Cerrar sesión" />
            <Button fx:id="ButtonConsultar" mnemonicParsing="false" text="Consultar" />
            <Button fx:id="buttonTransferir" mnemonicParsing="false" prefHeight="27.0" prefWidth="91.0" text="Transferir" />
         </children>
      </HBox>
   </children>
</VBox>
